#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/devshugo/shopify_lottery_draw/frontend/node_modules/.pnpm/vite@7.1.3_jiti@2.5.1_sass@1.79.3_tsx@4.19.2_yaml@2.8.1/node_modules/vite/bin/node_modules:/home/devshugo/shopify_lottery_draw/frontend/node_modules/.pnpm/vite@7.1.3_jiti@2.5.1_sass@1.79.3_tsx@4.19.2_yaml@2.8.1/node_modules/vite/node_modules:/home/devshugo/shopify_lottery_draw/frontend/node_modules/.pnpm/vite@7.1.3_jiti@2.5.1_sass@1.79.3_tsx@4.19.2_yaml@2.8.1/node_modules:/home/devshugo/shopify_lottery_draw/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/devshugo/shopify_lottery_draw/frontend/node_modules/.pnpm/vite@7.1.3_jiti@2.5.1_sass@1.79.3_tsx@4.19.2_yaml@2.8.1/node_modules/vite/bin/node_modules:/home/devshugo/shopify_lottery_draw/frontend/node_modules/.pnpm/vite@7.1.3_jiti@2.5.1_sass@1.79.3_tsx@4.19.2_yaml@2.8.1/node_modules/vite/node_modules:/home/devshugo/shopify_lottery_draw/frontend/node_modules/.pnpm/vite@7.1.3_jiti@2.5.1_sass@1.79.3_tsx@4.19.2_yaml@2.8.1/node_modules:/home/devshugo/shopify_lottery_draw/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../vite@7.1.3_jiti@2.5.1_sass@1.79.3_tsx@4.19.2_yaml@2.8.1/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../vite@7.1.3_jiti@2.5.1_sass@1.79.3_tsx@4.19.2_yaml@2.8.1/node_modules/vite/bin/vite.js" "$@"
fi
