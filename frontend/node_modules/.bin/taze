#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/devshugo/shopify_lottery_draw/frontend/node_modules/.pnpm/taze@19.3.0/node_modules/taze/bin/node_modules:/home/devshugo/shopify_lottery_draw/frontend/node_modules/.pnpm/taze@19.3.0/node_modules/taze/node_modules:/home/devshugo/shopify_lottery_draw/frontend/node_modules/.pnpm/taze@19.3.0/node_modules:/home/devshugo/shopify_lottery_draw/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/devshugo/shopify_lottery_draw/frontend/node_modules/.pnpm/taze@19.3.0/node_modules/taze/bin/node_modules:/home/devshugo/shopify_lottery_draw/frontend/node_modules/.pnpm/taze@19.3.0/node_modules/taze/node_modules:/home/devshugo/shopify_lottery_draw/frontend/node_modules/.pnpm/taze@19.3.0/node_modules:/home/devshugo/shopify_lottery_draw/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../taze/bin/taze.mjs" "$@"
else
  exec node  "$basedir/../taze/bin/taze.mjs" "$@"
fi
